<h1>New PickUp</h1>

<div class="row">
  <div class="col-md-8">
    <%= form_for [@location, @pick_up] do |f| %>
      <div class="form-group">
        <%= f.label :time %>
        <%= f.datetime_local_field :time, class: 'datetime_local' %>
      </div>
      <div class="form-group">
        <%= f.submit "Save", class: 'btn btn-success' %>
      </div>
    <% end %>
  </div>
</div>
