<h1>New PickUp</h1>

<div class="row">
  <div class="col-md-8">
    <%= simple_form_for [@location, @pick_up] do |f| %>
      <div class="form-group">
        <%= f.input :time, as: :datetime_local %>
      </div>
      <div class="form-group">
        <%= f.button :submit %>
      </div>
    <% end %>
  </div>
</div>
